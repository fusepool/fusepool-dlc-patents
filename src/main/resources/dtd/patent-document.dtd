<!-- General parameter entities
  Upper-level containers will carry these attributes. They will signify
  one or more sub-elements have changed or have been deleted within that
  container, e.g., classifications-ipc(r), inventors, applicants etc...
-->
<!ENTITY % dfltattrs "
  id ID #IMPLIED
  mxw-id CDATA #IMPLIED
  status (n|new|c|corrected|d|deleted) 'new'
  load-source CDATA #IMPLIED
  ref-ucid CDATA #IMPLIED
">

<!-- The contents of a paragraph or other block-level text element -->
<!ENTITY % ptext "
#PCDATA | b | i | u | o | sub | sup |smallcaps |
br | pre | dl | ul | ol | sl | crossref | figref | patcit |
nplcit | bio-deposit | img | imgref | chemistry | maths | tables | table-external-doc
">

<!-- The contents of headings or other block-level elements -->
<!ENTITY % btext "#PCDATA | img | imgref | b | i | u | o | sub | sup | smallcaps" >


<!-- External dependenies
 These include Table and Math markup and all character references.
-->
<!-- The ST32 MATH and CHEM definitions -->
<!ENTITY % st32 SYSTEM "st32-merges.dtd" >
%st32;

<!-- Namespaces and elements defined above in st32-merges.dtd -->
<!ENTITY % st32math "st32:df | st32:dfg | st32:f">
<!ENTITY % st32chem "st32:che | st32:chr | st32:crf">
<!ENTITY % extdep PUBLIC "-//FV//DTD Ext 1.0//EN" "ext-dependencies.dtd">
%extdep;


<!ELEMENT patent-document (doc-page+ |
                           (bibliographic-data,
                            abstract*,
                            description*,
                            claims*, amended-claims*, amended-claims-statement*,
                            drawings?,
                            search-report-data*,
                            legal-status?,
                            revision-history?, copyright?))>

<!--
  @ucid: unique identifier
  @file: internal (used by data provider)
  @lang: language of publication
  @country: originating office
  @doc-number: document publication number
  @kind: document kind code
  @date: date of publication
  @date-produced: internal (used by data provider)
  @family-id: DOCDB family identifer (use for grouping simple families)
  @file-reference-id: internal (used by data provider)
  @withdrawn: withdrawn status of document (yes is only value used)
  @family-id: DOCDB-provided indicator of family relationship. Integer value
              used to group members of a simple family
-->
<!ATTLIST patent-document
  %dfltattrs;
  ucid CDATA #REQUIRED
  file CDATA #IMPLIED
  lang CDATA #IMPLIED
  country CDATA #REQUIRED
  doc-number CDATA #REQUIRED
  kind CDATA #IMPLIED
  date CDATA #IMPLIED
  date-produced CDATA #IMPLIED
  file-reference-id CDATA #IMPLIED
  withdrawn (yes|no) #IMPLIED
  family-id CDATA #IMPLIED
>

<!--
  Used in EP/WO data for search-report section. WO also uses
  this element for references in the description and (amended) claims
  section of the document.
  @he: height
  @wi: width
  @type: image foramt
  @alt: descriptive text
  @pp: page range
  @ppf: first page
  @ppl: last page
  @file: relative location of external file
  @color: color or black and white page
                NOTE: some US plant patents are color images
  @orientation: layout of page
-->
<!ELEMENT doc-page EMPTY>
<!ATTLIST doc-page
  %dfltattrs;
  he CDATA #REQUIRED
  wi CDATA #REQUIRED
  type (jpg|pdf|tif) #REQUIRED
  alt CDATA #IMPLIED
  pp CDATA #IMPLIED
  ppf CDATA #IMPLIED
  ppl CDATA #IMPLIED
  file CDATA #REQUIRED
  color (color|bw) #IMPLIED
  orientation (portrait|landscape) 'portrait'
>

<!ELEMENT bibliographic-data (publication-reference,
                              us-sir-flag?,
                              application-reference?,
                              priority-claims?,
                              dates-of-public-availability?,
                              rule-47-flag?,
                              term-of-grant?,
                              technical-data?,
                              related-documents?,
                              parties?,
                              international-convention-data?,
                              designated-states?,
                              office-specific-data?) >

<!ELEMENT office-specific-data (eptags|ustags)*>
<!ELEMENT eptags        (ep-no-a-document-published?,
                         ep-limitations?,
                         ep-petition-reviews?)>
<!ELEMENT ustags        EMPTY>

<!ELEMENT ep-no-a-document-published   (#PCDATA)>

<!--
  @ucid: character identifier made up of country + document-number + kind,
         e.g., US-5551212-A; EP-0700000-B1
  @fvid: (internal) integer identifer used for QA/QC procedures
-->
<!ELEMENT publication-reference (document-id+)>
<!ATTLIST publication-reference
  %dfltattrs;
  ucid CDATA #REQUIRED
  fvid CDATA #REQUIRED
>

<!--
  @appl-type: US application type. This is reserverd for US only
              but other values may be used in the future.
  @us-series-code: US-only
  @us-art-unit: US-only
  @is-representative: see DOCDB ST36
-->
<!ELEMENT application-reference (document-id+) >
<!ATTLIST application-reference
  %dfltattrs;
  ucid CDATA #IMPLIED
  appl-type CDATA #IMPLIED
  us-series-code CDATA #IMPLIED
  us-art-unit CDATA #IMPLIED
  is-representative (YES|NO) "NO"
>

<!ELEMENT priority-claims (priority-claim+) >
<!ATTLIST priority-claims
  %dfltattrs;
>

<!ELEMENT priority-claim (document-id, office-of-filing?, (priority-doc-requested | priority-doc-attached)?) >
<!ATTLIST priority-claim
  %dfltattrs;
  ucid CDATA #IMPLIED
  kind (national|regional|international) #IMPLIED
>
<!ELEMENT priority-doc-attached EMPTY>
<!ELEMENT priority-doc-requested EMPTY>


<!ELEMENT rule-47-flag EMPTY >

<!ELEMENT term-of-grant (text | disclaimer | length-of-grant | us-term-extension | lapse-of-patent)*>
<!ATTLIST term-of-grant
  %dfltattrs;
>

<!ELEMENT length-of-grant (#PCDATA) >
<!ELEMENT us-term-extension (#PCDATA) >
<!ELEMENT disclaimer (date) >
<!ATTLIST disclaimer
  text CDATA #FIXED "This patent is subject to a terminal disclaimer"
>
<!ELEMENT lapse-of-patent (document-id, text?) >
<!ATTLIST lapse-of-patent
  %dfltattrs;
>




<!--
  With the exception of gazette-date, one of the following dates
  will be identical to publication-reference/date.
  Individual usage depends on publication-reference/kind
  
  US (1836-1999):
      A, S, P
      H, E, I    (granted-publication-date)
      
     (2000-Present):
      B1, B2,
      P2, P3
      S1, E1
      H1         (granted-publication-date)
      
      A1, P1     (application-publication)
      A2         (republication-date)
      A9         (corrected-publication-date)

  EP
      A1, A2     (application-publication-date)
      A3         (search-report-publication-date)
      A8         (amended-publication-date)
      A9         (corrected-publication-date)
      B1         (grant-publication-date)
      B2         (republication-date)
      B8         (amended-publication-date)
      B9         (corrected-publication-date)
  
  WO
      A1, A2     (application-publication-date)
      A3         (search-report-publication-date)
      A8         (amended-publication-date)
      A9         (corrected-publication-date)
-->
<!ELEMENT dates-of-public-availability (gazette-date?,
                                        application-publication-date?,
                                        grant-publication-date?,
                                        intention-to-grant-date?,
                                        registered-date?,
                                        republication-date?,
                                        search-report-publication-date?,
                                        search-report-dispatch-date?,
                                        corrected-search-report-dispatch-date?,
                                        coming-into-force-date?,
                                        preceding-publication-date?,
                                        amended-publication-date?,
                                        corrected-publication-date?) >
<!ATTLIST dates-of-public-availability
  %dfltattrs;
>
<!ELEMENT gazette-date (date, bulletin-number?)>
<!ELEMENT application-publication-date (date, bulletin-number?) >
<!ELEMENT grant-publication-date (date, bulletin-number?) >
<!ELEMENT intention-to-grant-date (date, bulletin-number?) >
<!ELEMENT registered-date (date, bulletin-number?) >
<!ELEMENT republication-date (date, bulletin-number?) >
<!ELEMENT search-report-publication-date (date, bulletin-number?) >
<!ELEMENT search-report-dispatch-date (date, bulletin-number?) >
<!ELEMENT corrected-search-report-dispatch-date (date, bulletin-number?) >
<!ELEMENT coming-into-force-date (date, bulletin-number?) >
<!ELEMENT preceding-publication-date (date, bulletin-number?) >
<!ELEMENT amended-publication-date (date, bulletin-number?) >
<!ELEMENT corrected-publication-date (date, bulletin-number?) >


<!ELEMENT technical-data ((classification-ipc | classification-locarno)*,
                          classifications-ipcr?, classification-national?,
                          classification-ecla*, f-term-info?,
                          invention-title*,
                          citations?,
                          figures?, number-of-claims?, exemplary-claim?,
                          field-of-search?)>
<!ATTLIST technical-data
  %dfltattrs;
>


<!ELEMENT classification-ipc (edition,
                              main-classification,
                              further-classification*,
                              (additional-info | linked-indexing-code-group | unlinked-indexing-code)*,
                              text?) >
<!ATTLIST classification-ipc
  %dfltattrs;
>
<!ELEMENT edition (#PCDATA)>
<!ELEMENT main-classification (#PCDATA) >
<!ATTLIST main-classification
  %dfltattrs;
  jp-facet CDATA #IMPLIED
>
<!ELEMENT further-classification (#PCDATA) >
<!ATTLIST further-classification
  %dfltattrs;
  jp-facet CDATA #IMPLIED
>
<!ELEMENT additional-info (#PCDATA) >
<!ATTLIST additional-info
  %dfltattrs;
>
<!ELEMENT linked-indexing-code-group (#PCDATA) >
<!ATTLIST linked-indexing-code-group
  %dfltattrs;
>
<!ELEMENT unlinked-indexing-code (#PCDATA) >
<!ATTLIST unlinked-indexing-code
  %dfltattrs;
>

<!--For US design patents only. -->
<!ELEMENT classification-locarno (edition?,
                                  main-classification,
                                  further-classification*,
                                  text?)>


<!ELEMENT classifications-ipcr (classification-ipcr+) >
<!ELEMENT classifications-ipcr-original (classification-ipcr+) >
<!--
<!ATTLIST classifications-ipcr-original
  as-of-date CDATA #REQUIRED
>
-->

<!ELEMENT classifications-ipcr-current (classification-ipcr+) >

<!ELEMENT classification-ipcr (#PCDATA) >
<!ATTLIST classification-ipcr
  %dfltattrs;
  sequence CDATA #IMPLIED
>

<!--
  This element will appear mainly in US data and DOCDB (US, GB and CA).
  US data will make use of '(main|further)-classification' and
  DOCDB will be limited to the 'text' child. Where DOCDB delivers
  US classes (country=US) and there are classifications from USPTO,
  USPTO classes will be preferred and delivered and DOCDB US classes
  will be suppressed.
-->
<!ELEMENT classification-national (edition?, main-classification?, further-classification*,
    (additional-info | linked-indexing-code-group | unlinked-indexing-code)*, text*)>
<!ATTLIST classification-national
  %dfltattrs;
  country CDATA #REQUIRED
>

<!ELEMENT classification-national-original (classification-national+) >
<!ELEMENT classification-national-current  (classification-national+) >


<!ELEMENT classification-ecla (classification-symbol+)>
<!ATTLIST classification-ecla
  %dfltattrs;
>

<!ELEMENT classification-symbol (#PCDATA) >
<!ATTLIST classification-symbol
  %dfltattrs;
  country CDATA #IMPLIED
  scheme (EC|ICO|IDT|ECNO) #REQUIRED
>

<!ELEMENT f-term-info (f-terms+)>
<!ELEMENT f-terms (theme-code, f-term+)>
<!ELEMENT f-term (#PCDATA)>
<!ATTLIST f-term
  %dfltattrs;
>
<!ELEMENT theme-code (#PCDATA) >



<!ELEMENT invention-title (#PCDATA) >
<!ATTLIST invention-title
  %dfltattrs;
  lang CDATA #REQUIRED
>

<!--
  Citations
-->

<!ELEMENT sources (source+) >
<!ELEMENT source EMPTY >
<!ATTLIST source
  %dfltattrs;
  name (APP|SEA|115|EXA|OPP|UNKNOWN) #REQUIRED
  created-by-npl (Y|N) "N"
  category CDATA #IMPLIED
>

<!ELEMENT citations (patent-citations?, non-patent-citations?) >
<!ELEMENT patent-citations (patcit+) >
<!ELEMENT patcit (text | (document-id, sources?, rel-passage*)) >
<!ATTLIST patcit
  %dfltattrs;
  ucid CDATA #IMPLIED
  num CDATA #IMPLIED
  url CDATA #IMPLIED
  dnum CDATA #IMPLIED
  dnum-type CDATA #IMPLIED
>

<!ELEMENT non-patent-citations (nplcit+) >
<!ELEMENT nplcit (#PCDATA | text | sources)* >
<!ATTLIST nplcit 
  %dfltattrs;
  num CDATA #IMPLIED
  url CDATA #IMPLIED
  npl-type CDATA #IMPLIED
>

<!ELEMENT rel-passage (text | (passage, (category, rel-claims?)*)) >
<!ELEMENT passage (#PCDATA) >
<!ELEMENT category (#PCDATA) >
<!ELEMENT rel-claims (#PCDATA) >

<!-- information about drawings and figures -->
<!ELEMENT figures (number-of-drawing-sheets?, number-of-figures?, figure-to-publish?)>
<!ELEMENT number-of-drawing-sheets (#PCDATA)>
<!ELEMENT number-of-figures (#PCDATA)>
<!ELEMENT figure-to-publish (#PCDATA)>

<!-- claims information -->
<!ELEMENT number-of-claims (#PCDATA)>
<!ELEMENT exemplary-claim (#PCDATA)>

<!-- search classifications -->
<!ELEMENT field-of-search (classification-national+) >
<!ATTLIST field-of-search
